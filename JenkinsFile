pipeline {
	agent any
	stages {
		stage ("Git") {
			steps {
				git branch: 'main',
				credentialsId: 'JenkinsSSH',
				url: 'https://github.com/CyrilGuissard/JenkinsCodeTest.git'
			}
		}
		stage ("Bandit Test") {
			steps {
				sh 'bandit -r $PWD > $WORKSPACE/reports/factorial.txt'
			}
		}
	}
}
